    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "husky": "^8.0.3",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "lint-staged": "^15.5.2",
    "prettier": "^3.6.2",
    "rollup-plugin-visualizer": "^6.0.3",
    "ts-jest": "^29.4.4",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
